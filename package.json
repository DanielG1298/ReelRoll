{
  "type": "module",
  "scripts": {
    "dev:Backend": "node Backend/server.js",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run dev:Backend\" \"npm run dev:frontend\"",
    "seed": "node -r dotenv/config Backend/db/seed.js",
    "test": "vitest",
    "db:schema": "psql -d reel_roll_db -f Backend/db/schema.sql",
    "db:reset": "npm run db:schema && npm run seed"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "supertest": "^7.1.4",
    "vitest": "^4.0.16"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
